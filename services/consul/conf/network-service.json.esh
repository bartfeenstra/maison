{
    "service": {
        "name": "network",
        "tags": ["maison-web", "maison-web-https", "maison-web-auth"],
        "address": "<%= $MAISON_LOCAL_IP %>",
        "port": 8443,
        "checks": [
            {
                "http": "https://<%= $MAISON_LOCAL_IP %>:8443",
                "tls_skip_verify": true,
                "interval": "10s"
            }
        ]
    }
}
