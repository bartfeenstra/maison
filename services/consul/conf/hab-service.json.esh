<% container_ip=$(docker inspect -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}' maison_hab_1) %>
{
    "service": {
        "name": "hab",
        "tags": ["maison", "web"],
        "meta": {
            "maison_proxy": "http://<%= $container_ip %>:8081",
            "maison_auth": "basic"
        },
        "checks": [
            {
                "http": "http://<%= $container_ip %>:8081",
                "interval": "10s"
            }
        ]
    }
}
