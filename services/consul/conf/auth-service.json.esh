<% container_ip=$(docker inspect -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}' maison_auth_1) %>
{
    "service": {
        "name": "auth",
        "tags": ["maison-web"],
        "address": "<%= $container_ip %>",
        "port": 8080,
        "checks": [
            {
                "http": "http://<%= $container_ip %>:8080",
                "interval": "2s"
            }
        ]
    }
}
