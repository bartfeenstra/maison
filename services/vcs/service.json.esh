<% container_ip=$(docker inspect -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}' maison_vcs_1) %>
{
    "Name": "vcs",
    "Tags": ["maison-web"],
    "Address": "<%= $container_ip %>",
    "Port": 3000,
    "Check": {
        "HTTP": "http://<%= $container_ip %>:3000",
        "Interval": "2s"
    }
}
