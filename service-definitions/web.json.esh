<% container_ip=$(docker inspect -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}' maison_web_1) %>
{
    "name": "web-http",
    "address": "<%= $container_ip %>",
    "port": 80,
    "checks": [
        {
            "tcp": "<%= $container_ip %>:80",
            "interval": "2s"
        }
    ]
}
